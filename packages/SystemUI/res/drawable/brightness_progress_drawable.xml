<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item android:id="@*android:id/background" android:gravity="center_vertical|fill_horizontal">
<shape android:shape="rectangle">
<size android:height="5dp" />
<solid android:color="#33000c29" />
<corners android:bottomRightRadius="0dp" android:bottomLeftRadius="5dp" android:topLeftRadius="0dp" android:topRightRadius="5dp"/>
</shape></item>
<item android:id="@*android:id/progress" android:gravity="center_vertical|fill_horizontal">
<scale android:scaleWidth="100%"><shape android:shape="rectangle">
<size android:height="5dp" />
    <gradient
        android:startColor="@*android:color/gradient_start"
        android:endColor="@*android:color/gradient_end"
        android:angle="45" android:type="linear"/>
<corners android:bottomRightRadius="0dp" android:bottomLeftRadius="5dp" android:topLeftRadius="0dp" android:topRightRadius="5dp"/>

</shape></scale></item></layer-list>
