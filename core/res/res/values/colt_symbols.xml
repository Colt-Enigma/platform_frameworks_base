<resources>
<!-- Whether to cancel fingerprint operation if not idle -->
    <java-symbol type="bool" name="config_fpCancelIfNotIdle" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display"/>

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Proximity check on screen on -->
    <java-symbol type="bool" name="config_proximityCheckOnWake" />

    <!-- Proximity check timeout -->
    <java-symbol type="integer" name="config_proximityCheckTimeout" />

    <!-- Proximity check on screen on default -->
    <java-symbol type="bool" name="config_proximityCheckOnWakeEnabledByDefault" />

    <!-- Advanced reboot -->
    <java-symbol type="string" name="reboot_to_recovery_title" />
    <java-symbol type="string" name="reboot_to_recovery_message" />
    <java-symbol type="string" name="reboot_to_bootloader_title" />
    <java-symbol type="string" name="reboot_to_bootloader_message" />
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_message" />

    <!-- Global Actions pref list -->
    <java-symbol type="array" name="custom_config_globalActionsList" />

    <!-- Sensor block -->
    <java-symbol type="array" name="config_blockPackagesSensorDrain" />

    <!-- For dynamic default font styles -->
    <java-symbol type="string" name="config_bodyFontFamily" />

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Whether device has dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- Parallel space badge -->
    <java-symbol type="string" name="parallel_space_badge_label" />
    <java-symbol type="drawable" name="ic_parallel_badge" />
    <java-symbol type="drawable" name="ic_parallel_icon_badge" />

    <!-- Parallel space badge colors --> 
    <java-symbol type="color" name="parallel_badge_1" />
    <java-symbol type="color" name="parallel_badge_2" />
    <java-symbol type="color" name="parallel_badge_3" />
    <java-symbol type="color" name="parallel_badge_4" />
    <java-symbol type="color" name="parallel_badge_5" />
    <java-symbol type="color" name="parallel_badge_6" />
    <java-symbol type="color" name="parallel_badge_7" />
    <java-symbol type="color" name="parallel_badge_8" />
    <java-symbol type="color" name="parallel_badge_9" />

    <!-- Parallel space max count. -->
    <java-symbol type="integer" name="config_parallelSpaceMaxCount" />

    <!-- Enable face auth only when swiping security view -->
    <java-symbol type="bool" name="config_faceAuthOnlyOnSecurityView" />

</resources>
